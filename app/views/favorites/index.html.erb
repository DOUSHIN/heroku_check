<h1><%= current_user.name %>のお気に入りのページ</h1>
<%= link_to 'New Post', new_picture_path %>
<%= link_to 'All Posts', pictures_path %>

<table class="main_table">
    <% @favorites.each do |favorite| %>
      <tr>
        <td><%= favorite.picture.user.name %></td>
        <td><%= favorite.picture.content %></td>
        <td><%= image_tag favorite.picture.image.url %></td>
      </tr>
    <% end %>
</table>

<br>
